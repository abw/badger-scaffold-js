{
  "name": "{{dist}}",
  "version": "0.0.1",
  "description": "A description for {{name}}",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/{{name}}.umd.cjs",
  "module": "./dist/{{name}}.js",
  "exports": {
    ".": {
      "import": "./dist/{{name}}.js",
      "require": "./dist/{{name}}.umd.cjs"
    }
  },
  "author": "{{author}}",
  "licence": "{{licence}}",
  "keywords": [
    "react"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:docs": "vite build --outDir docs --config vite.docs.js",
    "preview": "vite preview --outDir docs --config vite.docs.js",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/{{githubId}}/{{name}}.git"
  },
  "bugs": {
    "url": "https://github.com/{{githubId}}/{{name}}/issues"
  },
  "dependencies": {
{%- for module, version in dependencies %}
    "{{module}}": "{{version}}"{{',' if not loop.last }}
{%- endfor %}
  },
  "devDependencies": {
{%- for module, version in devDependencies %}
    "{{module}}": "{{version}}"{{',' if not loop.last }}
{%- endfor %}
  }
}
